<p id="notice"><%= notice %></p>

<h1>Og Previewers</h1>

<table>
  <thead>
    <tr>
      <th>Input url</th>
      <th>Og image url</th>
      <th>Job</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @og_previewers.each do |og_previewer| %>
      <tr>
        <td><%= og_previewer.input_url %></td>
        <td><%= og_previewer.og_image_url %></td>
        <td><%= og_previewer.job_id %></td>
        <td><%= og_previewer.status %></td>
        <td><%= link_to 'Show', og_previewer %></td>
        <td><%= link_to 'Destroy', og_previewer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Og Previewer', new_og_previewer_path %>
